{
  "compilerOptions": {
    "target": "ES2022",
    "module": "esnext",
    "moduleResolution": "bundler",
    "esModuleInterop": true,
    "strict": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "resolveJsonModule": true,
    "declaration": false,
    "outDir": "./dist",
    "rootDir": "./src",
    "lib": [
      "ES2022"
    ]
  },
  "include": [
    "src/**/*",
    ".eleventy.ts"
  ],
  "exclude": [
    "node_modules",
    "_site",
    "dist"
  ]
}